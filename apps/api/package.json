{
  "name": "@chatbot/api",
  "version": "1.0.0",
  "description": "API NestJS para ChatBot 2.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "echo 'Lint skipped for API'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "dotenv -e ../../.env -- prisma generate --schema prisma/schema.prisma",
    "db:push": "dotenv -e ../../.env -- prisma db push --schema prisma/schema.prisma",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate deploy --schema prisma/schema.prisma",
    "db:seed": "prisma db seed",
    "qa:seed": "ts-node -r tsconfig-paths/register src/qa/qa-seed.service.ts",
    "db:studio": "prisma studio",
    "clean": "rm -rf dist",
    "sec:scan": "powershell -ExecutionPolicy Bypass -File scripts/security-scan.ps1"
  },
  "dependencies": {
    "@chatbot/config": "workspace:*",
    "@chatbot/core": "workspace:*",
    "@chatbot/providers": "workspace:*",
    "@faker-js/faker": "^10.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.2.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.0",
    "@nestjs/platform-socket.io": "^10.2.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/websockets": "^10.2.0",
    "@prisma/client": "^5.7.0",
    "@types/argon2": "^0.15.4",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@whiskeysockets/baileys": "^6.7.19",
    "argon2": "^0.31.2",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jimp": "^1.6.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.17.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0"
  },
  "prisma": {
    "seed": "dotenv -e ../../.env -- ts-node prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
